{
	"name": "server",
	"version": "1.0.0",
	"scripts": {
		"dev": "ts-node-dev --transpile-only --no-notify -r tsconfig-paths/register index.ts",
		"start": "ts-node --transpile-only -r tsconfig-paths/register index.ts",
		"build": "tsc --build tsconfig.json",
		"seed": "ts-node --transpile-only -r tsconfig-paths/register scripts/seed.ts",
		"clear": "ts-node --transpile-only -r tsconfig-paths/register scripts/clear.ts",
		"populateAssets": "ts-node --transpile-only -r tsconfig-paths/register scripts/populateAssets.ts",
		"test": "jest --detectOpenHandles --coverage",
		"test:watch": "jest --detectOpenHandles --watchAll",
		"script": "ts-node --transpile-only -r tsconfig-paths/register"
	},
	"dependencies": {
		"@nexus/schema": "^0.20.1",
		"@types/multer": "^1.4.7",
		"apollo-server": "^3.3.0",
		"apollo-server-express": "^3.5.0",
		"apollo-server-plugin-base": "^3.3.0",
		"axios": "^0.24.0",
		"bson": "^4.5.2",
		"dotenv": "^10.0.0",
		"express": "^4.17.1",
		"graphql": "^15.8.0",
		"graphql-iso-date": "^3.6.1",
		"jsonwebtoken": "^8.5.1",
		"mongodb": "^4.1.2",
		"mongoose": "^6.0.7",
		"multer": "^1.4.4",
		"nexus": "^1.1.0",
		"node-cache": "^5.1.2",
		"qs": "^6.10.3",
		"ts-node": "^10.4.0",
		"tsconfig-paths": "^3.11.0",
		"winston": "^3.3.3"
	},
	"devDependencies": {
		"@types/jest": "^27.0.3",
		"jest": "^27.4.5",
		"mongodb-memory-server": "^8.1.0",
		"ts-jest": "^27.1.2",
		"ts-node-dev": "^1.1.8"
	}
}