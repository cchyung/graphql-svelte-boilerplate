{
	"name": "client",
	"version": "0.0.1",
	"scripts": {
		"dev": "concurrently \"svelte-kit dev\" \" npm run gql:typegen:watch\" --prefix=none",
		"build": "svelte-kit build --verbose",
		"install-and-fix": "./scripts/pre-build.sh",
		"preview": "svelte-kit preview --port=3001",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. .",
		"format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
		"gql:typegen": "graphql-codegen --config codegen.yml",
		"gql:typegen:watch": "graphql-codegen --config codegen.yml --watch"
	},
	"devDependencies": {
		"@babel/core": "^7.16.10",
		"@graphql-codegen/add": "^3.1.1",
		"@graphql-codegen/cli": "^2.2.0",
		"@graphql-codegen/typescript": "^2.2.2",
		"@graphql-codegen/typescript-operations": "^2.1.5",
		"@rollup/plugin-commonjs": "^21.0.1",
		"@rollup/plugin-node-resolve": "^13.1.3",
		"@sveltejs/adapter-vercel": "next",
		"@sveltejs/kit": "1.0.0-next.260",
		"@types/canvas-confetti": "^1.4.2",
		"@types/cookie": "^0.4.1",
		"@zerodevx/svelte-toast": "^0.7.1",
		"autoprefixer": "^10.4.1",
		"babel-loader": "^8.2.3",
		"postcss": "^8.4.5",
		"postcss-import": "^14.0.2",
		"postcss-load-config": "^3.1.3",
		"prettier": "^2.4.1",
		"prettier-plugin-svelte": "^2.6.0",
		"rollup-plugin-polyfill-node": "^0.7.0",
		"sass": "^1.42.1",
		"svelte": "^3.49.0",
		"svelte-check": "^2.2.6",
		"svelte-loader": "^3.1.2",
		"svelte-meta-tags": "^2.6.0",
		"svelte-preprocess": "^4.9.4",
		"tailwindcss": "^3.0.10",
		"tslib": "^2.3.1",
		"typescript": "^4.4.3"
	},
	"type": "module",
	"dependencies": {
		"@apollo/client": "^3.6.0-beta.5",
		"@rollup/plugin-inject": "^4.0.4",
		"@tailwindcss/forms": "^0.4.0",
		"@tailwindcss/line-clamp": "^0.4.0",
		"animejs": "^3.2.1",
		"axios": "^0.26.0",
		"buffer": "^6.0.3",
		"canvas-confetti": "^1.5.1",
		"dompurify": "^2.3.6",
		"graphql": "^15.8.0",
		"is-ipfs": "^6.0.2",
		"jsonwebtoken": "^8.5.1",
		"linkify-html": "^3.0.5",
		"linkify-plugin-hashtag": "^3.0.4",
		"linkify-plugin-mention": "^3.0.4",
		"linkifyjs": "^3.0.5",
		"marked": "^4.0.14",
		"prettier-plugin-svelte": "^2.6.0",
		"qs": "^6.10.3",
		"svelte-file-dropzone": "^1.0.0-dev.1"
	}
}